{
  "name": "hapi-architect",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node ./dist/main.js",
    "lint": "eslint ./src/** ./tests/**",
    "lint:fix": "eslint --fix ./src/** ./tests/**",
    "dev": "nodemon -e ts,json --watch ./src --ignore ./src/**/*.spec.ts --exec node -r ts-node/register ./src/main.ts",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect -r ts-node/register ./src/main.ts",
    "typeorm": "typeorm-ts-node-esm -d src/database/ormConfig.ts",
    "test": "jest",
    "test:e2e": "jest --config ./tests/jest.config.js"
  },
  "keywords": [
    "hapi",
    "typeorm",
    "inversify",
    "jest",
    "typescript"
  ],
  "author": "Alexey Mytsykov",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@hapi/hapi": "^21.3.1",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "hapi-auth-jwt2": "^10.4.0",
    "hapi-pino": "^12.0.0",
    "inversify": "^6.0.1",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.15"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
